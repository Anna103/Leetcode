    "\n",
    "def findMin(nums):\n",
    "    min_num = nums[0]\n",
    "    for i in range(1,len(nums)):\n",
    "        if(min_num > nums[i]):\n",
    "            min_num = nums[i]\n",
    "    return min_num"
   ]
